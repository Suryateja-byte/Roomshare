{
  "name": "roomshare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate && node -e \"require('fs').copyFileSync('node_modules/mapbox-gl/dist/mapbox-gl-csp-worker.js','public/mapbox-gl-csp-worker.js')\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "verify": "node verify.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest --testPathPatterns=\"src/__tests__/(lib|hooks|utils)/\" --testPathIgnorePatterns=\"filter-regression\"",
    "test:components": "jest --testPathPatterns=\"src/__tests__/(components|pages)/\"",
    "test:api": "jest --testPathPatterns=\"src/__tests__/(api|actions)/\"",
    "test:filters": "jest src/__tests__/lib/filter-schema.test.ts src/__tests__/integration/ src/__tests__/property/ src/__tests__/e2e/",
    "test:filters:schema": "jest src/__tests__/lib/filter-schema.test.ts",
    "test:filters:integration": "jest src/__tests__/integration/",
    "test:filters:property": "jest src/__tests__/property/",
    "test:filters:e2e": "jest src/__tests__/e2e/",
    "test:filters:regression": "jest src/__tests__/lib/filter-regression.test.ts",
    "test:filters:perf": "jest src/__tests__/performance/ --testTimeout=60000",
    "test:filters:all": "jest src/__tests__/lib/filter-schema.test.ts src/__tests__/lib/filter-regression.test.ts src/__tests__/integration/ src/__tests__/property/ src/__tests__/e2e/ src/__tests__/performance/ --testTimeout=60000",
    "clean:next-locks": "rm -rf .next/dev/lock 2>/dev/null || true",
    "seed:e2e": "node scripts/seed-e2e.js",
    "test:e2e": "pnpm run clean:next-locks && pnpm run seed:e2e && playwright test",
    "test:e2e:ci": "playwright test --project=chromium --reporter=list,html",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:anon": "playwright test --project=chromium-anon",
    "geocode:compare": "npx ts-node scripts/geocode-accuracy-check.ts",
    "geocode:compare:extended": "npx ts-node scripts/geocode-accuracy-extended.ts",
    "geocode:compare:json": "npx ts-node scripts/geocode-accuracy-extended.ts --json"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/openai": "^2.0.80",
    "@ai-sdk/react": "^2.0.109",
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/nextjs": "^10.30.0",
    "@supabase/supabase-js": "^2.84.0",
    "@types/bcryptjs": "^3.0.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "ai": "^5.0.108",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.25",
    "lightningcss": "^1.30.2",
    "lucide-react": "^0.554.0",
    "mapbox-gl": "^2.15.0",
    "maplibre-gl": "^5.15.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-map-gl": "^7.1.7",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "use-debounce": "^10.0.6",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@currents/playwright": "^1.21.1",
    "@napi-rs/canvas": "^0.1.88",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "fast-check": "^4.4.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "next-router-mock": "^1.0.4",
    "prisma": "^6.0.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
